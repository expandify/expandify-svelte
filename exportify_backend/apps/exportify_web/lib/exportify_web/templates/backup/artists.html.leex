<div class="top">
  <h1>Your Artists</h1>
  <h2>You follow <%= length(@artists) %> Artists.</h2>
</div>

<div class="bottom bottom-artists">
  <%= for artist <- @artists do %>
    <div class="artist">
      <div class="image">
        <div class="content"> <img src="<%= artist.image %>" alt=""/> </div>
      </div>

      <div class="name">
        <%= artist.name %>
      </div>
    </div>
  <% end %>
</div>